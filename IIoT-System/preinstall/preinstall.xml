<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>preinstall</groupId>
    <artifactId>ml-jars</artifactId>
    <version>1.0</version>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-install-plugin</artifactId>
                <version>2.5.2</version>
                <executions>
                    <execution>
                        <phase>initialize</phase>
                        <id>javaml</id>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>javaml</artifactId>
                            <version>0.1.5</version>
                            <file>${project.basedir}/src/main/resources/javaML/javaml-0.1.5.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <id>ajt</id>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>ajt</artifactId>
                            <version>1.20</version>
                            <file>${project.basedir}/src/main/resources/javaML/ajt-1.20.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <id>math</id>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>commons-math</artifactId>
                            <version>1.2</version>
                            <file>${project.basedir}/src/main/resources/javaML/commons-math-1.2.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <id>Jama</id>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>Jama</artifactId>
                            <version>1.0.2</version>
                            <file>${project.basedir}/src/main/resources/javaML/Jama-1.0.2.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <id>libsvm</id>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>libsvm</artifactId>
                            <version>1.0</version>
                            <file>${project.basedir}/src/main/resources/javaML/libsvm.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                    <execution>
                        <phase>initialize</phase>
                        <goals>
                            <goal>install-file</goal>
                        </goals>
                        <id>weka</id>
                        <configuration>
                            <groupId>net.sourceforge</groupId>
                            <artifactId>weka</artifactId>
                            <version>1.0</version>
                            <file>${project.basedir}/src/main/resources/javaML/weka.jar</file>
                            <packaging>jar</packaging>
                        </configuration>
                    </execution>
                </executions>
            </plugin>
        </plugins>
    </build>
</project>
